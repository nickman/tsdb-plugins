{
     "name": "TSDBConsole",
     "description": "Management Console for OpenTSDB",
     "version": "1.0",
     "app": {
          "background": {
               "scripts": ["main.js"]
          }
     },
     "icons": {
          "128": "/img/tsdbicon_128.png",
          "16": "/img/tsdbicon_16.png"
     },
     "permissions": [
          "clipboardRead",
          "clipboardWrite",
          "notifications",
          "syncFileSystem",
          "unlimitedStorage",
          "storage",
          "tts",
          "contextMenus",
          "fileBrowserHandler",
          "idle",
          "webview",
          "alarms",
          "<all_urls>"

     ],
     "manifest_version": 2
}


