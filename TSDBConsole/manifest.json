{
     "name": "TSDBConsole",
     "description": "Management Console for OpenTSDB",
     "version": "1.0",
     "webview": {
          "partitions": [     
               {
                    "name": "servers",
                    "accessible_resources": ["/asynch/server/asynch.*", "/js/jquery/jquery-2.0.3.js"]
               }
          ]
     },



     "app": {
          "background": {
               
               "page": "/asynch/server/asynchpage.html"

          }
     },
     "icons": {
          "128": "/img/tsdbicon_128.png",
          "16": "/img/tsdbicon_16.png"
     },
     "sandbox": {
          "pages": ["/asynch/server/asynch.html"]       
     },
     "externally_connectable": {
       "matches": ["*://mgonnefpincaneibcfpncbifgllnijli/*"]
     },
     "permissions": [
          "clipboardRead",
          "clipboardWrite",
          "notifications",
          "syncFileSystem",
          "unlimitedStorage",
          "storage",
          "tts",
          "contextMenus",
          "fileBrowserHandler",
          "idle",
          "webview",
          "alarms",
          "<all_urls>"

     ],
     "manifest_version": 2
}


