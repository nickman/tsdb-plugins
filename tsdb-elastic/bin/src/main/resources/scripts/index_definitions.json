{
   "tsdb-elastic-index-mapping":{
      "TSMeta":{
         "index":{
            "name":"opentsdb",
            "settings":{
               "compound_format":false,
               "compound_on_flush":true,
               "number_of_replicas":1,
               "number_of_shards":5,
               "refresh_interval":1,
               "term_index_divisor":1,
               "term_index_interval":128
            }
         },
         "mapping":{
            "tsmeta":{
               "properties":{
                  "metric":{
                     "properties":{
                        "name":{
                           "index":"not_analyzed",
                           "type":"string"
                        }
                     }
                  },
                  "tags":{
                     "properties":{
                        "name":{
                           "index":"not_analyzed",
                           "type":"string"
                        },
                        "uid":{
                           "index":"not_analyzed",
                           "type":"string"
                        }
                     }
                  },
                  "tsuid":{
                     "index":"not_analyzed",
                     "type":"string"
                  }
               }
            }
         }
      },
      "UIDMeta":{
         "index":{
            "name":"opentsdb",
            "settings":{
               "compound_format":false,
               "compound_on_flush":true,
               "number_of_replicas":1,
               "number_of_shards":5,
               "refresh_interval":1,
               "term_index_divisor":1,
               "term_index_interval":128
            }
         },
         "mapping":{
            "uidmeta":{
               "properties":{
                  "uid":{
                     "type":"string",
                     "index":"not_analyzed"
                  },
                  "name":{
                     "type":"string",
                     "index":"not_analyzed"
                  }
               }
            }
         }
      }
   }
}